    "659ab707-9b07-433d-9944-6b0b1de6da81": {
      "type": "multi-column",
      "blocks": {
        "template--17692579594522__659ab707-9b07-433d-9944-6b0b1de6da81-1678087710c401100e-0": {
          "type": "item",
          "settings": {
            "image": "shopify://shop_images/Supporting_Stroller1_4-3_WEB.jpg",
            "column_size": "3",
            "title": "For strollers",
            "heading_tag": "h4",
            "content": "<p>The Sleepytroll baby rocker attaches to your stroller, providing a gentle rocking motion that helps soothe babies and extend those daytime naps. It is portable and lightweight, making it convenient to use while out and about.</p>",
            "link_url": "https://us.sleepytroll.com/pages/versitality",
